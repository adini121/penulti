publisher = {Ieee},
title = {{Mining temporal specifications from object usage}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=5431762 http://link.springer.com/article/10.1007/s10515-011-0084-1},
year = {2011}
}
